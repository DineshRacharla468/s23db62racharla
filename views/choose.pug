extends layout

block content
  h1= title
  - var image_names = ['img1.jpg','img2.jpeg','img3.jpg','img4.jpg','img5.jpg',]

  - for(let i = 0; i<3; i++)
    - let index = Math.floor(Math.random()*5)
    img(src=`/images/${ image_names[index]}`, alt=`${ image_names[index]}`, height="300", width="300")